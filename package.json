{
  "name": "ertsv-app",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/index.js",
  "scripts": {
    "build:main": "tsc",
    "watch:main": "tsc --watch",
    "dev:vite": "vite",
    "start:electron": "wait-on tcp:5173 && electron .",
    "dev": "npm run build:main && concurrently -k \"npm:watch:main\" \"npm:dev:vite\" \"npm:start:electron\""
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.2.0",
    "electron": "^31.2.1",
    "typescript": "^5.4.5",
    "vite": "^7.0.6",
    "wait-on": "^8.0.4"
  }
}